<% content_for :meta_title, t('.title') %>
<% content_for :options do %>
    <ul>
      <li><%= link_to t(:create), new_theme_path %></li>
      <li><%= link_to t(:view), @entity %></li>
      <li><%= link_to t(:list), themes_path %></li>
      <li><%= link_to_delete @entity %></li>
    </ul>
<% end %>

<article class="edit-theme">
  <h1><%= t('.title') %></h1>

  <%= render partial: 'themes/form', locals: { entity: @entity } %>
</article>
