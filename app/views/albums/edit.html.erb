<% content_for :meta_title, t('.title') %>
<% content_for :options do %>
    <ul>
      <li><%= link_to t(:create), new_album_path %></li>
      <li><%= link_to t(:view), @entity %></li>
      <li><%= link_to t(:list), albums_path %></li>
      <li><%= link_to_delete @entity %></li>
    </ul>
<% end %>

<article class="edit-album">
  <h1><%= t('.title') %></h1>

  <%= render partial: 'albums/form', locals: { entity: @entity } %>
</article>
